  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "down": "docker-compose down",
    "env": "export $(cat .env | xargs)",
    "knex": "knex --knexfile ./src/config/Knexfile.ts",
    "lint": "echo 'command not implemented'",
    "start": "yarn knex migrate:latest && node build/",
    "stoppit": "lsof -n -i4TCP:3000 | grep LISTEN | awk '{ print $2 }' | xargs kill",
    "test": "echo 'command not implemented'",
    "test:ci": "echo 'command not implemented'",
    "up": "yarn env && docker-compose up",
    "up:b": "yarn up --build",
    "up:d": "yarn up -d"
  },
